public class SignUpPage extends JFrame {

	
private JLabel cover,oritation;
private JButton homeJButton,helpJButton, aboutJButton, exitJButton;

private JButton createJButton, loginJButton ;
	


	
	public SignUpPage()
	{
		 createUserInterface();
		 
	}
	
	
	
	private void createUserInterface()
	{
		Container contentPane = getContentPane();
		contentPane.setLayout(null);
		contentPane.setBackground(Color.white);
		

		homeJButton = new JButton();
		homeJButton.setBounds(0,100,50,30);
		contentPane.add( homeJButton );
		
	
		oritation = new JLabel(new ImageIcon("menuEnhanced.gif"));
		oritation.setBounds(0,0,700,135);
		oritation.setVisible(true);
		contentPane.add( oritation );
		
		
		
		
		
		
		cover  = new JLabel(new ImageIcon("all"));
		cover.setBounds(0, -35, 904, 674 );
		cover.setVisible(false);
		contentPane.add( cover );
		
		
		setSize(695,587);
		setTitle("Welcome to Travel Planner System");
		setVisible(true);
		setResizable(true);
		
	}
	
	
	


}

